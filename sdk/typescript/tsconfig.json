{
  "compilerOptions": {
    /* https://www.typescriptlang.org/tsconfig */
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Node",
    "declaration": true,
    "outDir": "dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "alwaysStrict": true,
  },
  "exclude": [
    "dist",
    "input",
    "generated_ts",
    "out",
    ".scratch"
  ]
}